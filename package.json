{
  "name": "e-commerce",
  "version": "1.0.0",
  "main": "server.js",
  "author": "NodeJS",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "build": "yarn clean && babel ./src -d ./build/src",
    "production": "yarn build && node ./build/src/server.js",
    "start-docker": "docker-compose up -d",
    "start-server": "nodemon --exec ./node_modules/.bin/babel-node ./src/server.js",
    "stop-docker": "docker-compose down",
    "dev": "yarn run start-docker && sleep 1 && yarn run start-server"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.1.3",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "ioredis": "^5.3.2",
    "googleapis": "^120.0.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.3.3",
    "nodemailer": "^6.9.3",
    "node-schedule": "^2.1.1",
    "sequelize": "^6.32.0",
    "winston": "^3.9.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.5",
    "@babel/core": "^7.22.1",
    "@babel/node": "^7.22.1",
    "@babel/preset-env": "^7.22.4",
    "date-fns": "^2.30.0",
    "nodemon": "^2.0.22",
    "sequelize-cli": "^6.6.0"
  }
}
